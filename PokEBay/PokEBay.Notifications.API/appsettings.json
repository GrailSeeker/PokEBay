{
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "DaprConfiguration": {
    "AppId": "pokebaynotificationsapi",
    "StateStore": "pokebay-statestore",
    "StateStoreKey": "pokebay_notification",
    "PubSubComponent": "pubsub",
    "NotificationTopic": "NewOrderCreatedEvent",
    "EmailBindingStore": "sendmail"
  },
  "SwaggerConfiguration": {
    "ApiEndpoint": "/swagger/v1/swagger.json",
    "ApiName": "PokEBay.Notification.API",
    "ApiVersion": "v1"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
